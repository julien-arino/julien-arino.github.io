 doi = {10.1007/s00285-022-01765-9},
 google_scholar_id = {J-ba04ztB30C},
 journal = {Journal of Mathematical Biology},
 pdf = {https://link.springer.com/content/pdf/10.1007/s00285-022-01765-9.pdf},
 title = {Bistability in deterministic and stochastic SLIAR-type models with imperfect and waning vaccine protection},
