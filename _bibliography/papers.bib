 number = {6},
 pages = {198-204},
 pdf = {https://www.canada.ca/content/dam/phac-aspc/documents/services/reports-publications/canada-communicable-disease-report-ccdr/monthly-issue/2020-46/issue-6-june-4-2020/ccdrv46i06a08-eng.pdf},
 title = {Modelling scenarios of the epidemic of {COVID}-19 in {C}anada},
 url = {https://www.canada.ca/content/dam/phac-aspc/documents/services/reports-publications/canada-communicable-disease-report-ccdr/monthly-issue/2020-46/issue-6-june-4-2020/ccdrv46i06a08-eng.pdf},
