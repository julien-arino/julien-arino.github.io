 keywords = {Mathematical modelling; evidence synthesis; influenza; vaccine},
 month = {July},
 nlm-id = {101224383},
 owner = {NLM},
 pages = {11},
 pdf = {https://tbiomed.biomedcentral.com/track/pdf/10.1186/s12976-020-00129-4},
