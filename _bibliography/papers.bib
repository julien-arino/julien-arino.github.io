 pages = {368--376},
 pdf = {papers/2013_KhanRoEckhardtBrownstein_etal-BWHO91.pdf},
 timestamp = {2013.03.27},
 title = {Entry and exit screening of airline travellers during the {A(H1N1)} 2009 pandemic: a retrospective evaluation},
